{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-L${workspaceFolder}",
                "-g",
                "${workspaceFolder}/main.c",
                "-o",
                "${workspaceFolder}/dev/prog",
                "-lm"
                //"-lCH341DLL"
                //"-std=c++98"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "compiler: /usr/bin/gcc"
        }
    ],
    "version": "2.0.0"
}